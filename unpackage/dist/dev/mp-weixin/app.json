{
  "pages": [
    "pages/tabbar/home",
    "pages/tabbar/order",
    "pages/tabbar/user",
    "pages/permission/permission"
  ],
  "subPackages": [
    {
      "root": "pages/order",
      "pages": [
        "expert",
        "orderPay",
        "payResult",
        "orderDetail"
      ]
    }
  ],
  "usingComponents": {
    "van-button": "/wxcomponents/vant/button/index"
  },
  "window": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8",
    "backgroundTextStyle": "dark",
    "enablePullDownRefresh": true,
    "onReachBottomDistance": 50,
    "pageOrientation": "portrait"
  },
  "tabBar": {
    "borderStyle": "black",
    "backgroundColor": "#FFFFFF",
    "color": "#8a8a8a",
    "selectedColor": "#0088ff",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/tabbar/home",
        "iconPath": "static/images/tabbar/expret.png",
        "selectedIconPath": "static/images/tabbar/expret_active.png",
        "text": "专家"
      },
      {
        "pagePath": "pages/tabbar/order",
        "iconPath": "static/images/tabbar/message.png",
        "selectedIconPath": "static/images/tabbar/message_active.png",
        "text": "咨询"
      },
      {
        "pagePath": "pages/tabbar/user",
        "iconPath": "static/images/tabbar/user.png",
        "selectedIconPath": "static/images/tabbar/user_active.png",
        "text": "我"
      }
    ]
  },
  "preloadRule": {
    "pages/order/expert": {
      "network": "all",
      "packages": [
        "__APP__"
      ]
    }
  }
}